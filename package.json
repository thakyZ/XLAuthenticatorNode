{
  "name": "xlauthenticatornode",
  "version": "1.0.0",
  "description": "CLI TOTP Application for XL Launcher",
  "bin": {
    "xlauth": "bin/xlauth.js"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "engines": {
    "node": ">=18.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thakyZ/XLAuthenticatorNode.git"
  },
  "keywords": [
    "cli-app",
    "cli",
    "totp",
    "ffxiv",
    "xlauncher",
    "exe"
  ],
  "pkg": {
    "assets": "node_modules/**/*"
  },
  "help": "pkg . --target host --output XLAuthenticator-1.0.0-x64.exe --debug",
  "author": "Neko Boi Nick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thakyZ/XLAuthenticatorNode/issues"
  },
  "homepage": "https://github.com/thakyZ/XLAuthenticatorNode#readme",
  "dependencies": {
    "axios": "^1.1.3",
    "buffer": "^6.0.3",
    "command-line-args": "^5.2.1",
    "dotenv": "^16.0.3",
    "prompt": "^1.3.0",
    "totp-generator": "^0.0.14"
  },
  "devDependencies": {
    "ava": "^5.1.0",
    "eslint": "^8.28.0",
    "execa": "^5.0.0",
    "p-event": "^4.2.0",
    "xo": "^0.53.1"
  },
  "xo": {
    "semicolon": true,
    "space": 2,
    "rules": {
      "unicorn/prefer-module": "off",
      "quotes": [
        "error",
        "double"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "unicorn/no-process-exit": [
        "off"
      ]
    }
  }
}
